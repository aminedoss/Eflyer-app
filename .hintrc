{
  "extends": [
    "development"
  ],
  "hints": {
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "text-align: -webkit-match-parent",
          "-webkit-text-size-adjust",
          "print-color-adjust",
          "user-select",
          "mask-image",
          "mask-size",
          "mask-position",
          "-webkit-tap-highlight-color",
          "text-decoration-skip-ink",
          "-webkit-overflow-scrolling",
          "min-height: auto",
          "overflow-anchor"
        ]
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not samsung <= 25"
  ]
}